<div class="contents row">
  <div class="container">
    <h3>投稿する</h3>
    <%= form_with(model: @exhibition, local: true) do |form| %>
      <%= form.text_field :date, placeholder: "Date" %>
      <%= form.text_field :image, placeholder: "Image Url" %>
      <%= form.text_area :title, placeholder: "title", rows: "10" %>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
</div>